{
  "schemaVersion": 1,
  "id": "deicore",
  "version": "${version}",
  "name": "deicore",
  "description": "This is a Fabric mod for Minecraft, which adds commands such as WorldVar and some APIs",
  "authors": [],
  "contact": {},
  "license": "LGPL-3.0",
  "icon": "assets/deicore/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "org.dideng.com.deicore.client.datagen.DeicoreDataGenerator"
    ],
    "client": [
      "org.dideng.com.deicore.client.DeicoreClient"
    ],
    "main": [
      "org.dideng.com.deicore.Deicore"
    ]
  },
  "mixins": [
    "deicore.mixins.json",
    {
      "config": "deicore.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=1.21 <=1.21.1"
  }
}
